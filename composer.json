{
  "name": "t2cn/framework",
  "type": "library",
  "keywords": [
    "High Performance",
    "Http Service",
    "T2 Framework",
    "Workerman",
    "T2 Engine",
    "php"
  ],
  "homepage": "https://www.t2engine.cn",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "Tony",
      "email": "dev@t2engine.cn",
      "homepage": "https://www.t2engine.cn",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "dev@t2engine.cn",
    "issues": "https://github.com/t2cn/framework/issues",
    "forum": "https://forum.t2engine.cn/",
    "wiki": "https://wiki.t2engine.cn/",
    "source": "https://github.com/t2cn/framework"
  },
  "require": {
    "php": ">=8.3",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "workerman/workerman": "^5.0.0",
    "nikic/fast-route": "^1.3",
    "psr/container": ">=1.0",
    "psr/simple-cache": "^3.0",
    "monolog/monolog": "^2.0",
    "illuminate/database": "^11.38",
    "illuminate/pagination": "^11.38",
    "illuminate/events": "^11.38",
    "illuminate/redis": "^11.38",
    "symfony/cache": "^7.2",
    "twig/twig": "^3.18"
  },
  "require-dev": {
    "ext-zend-opcache": "*",
    "ext-simplexml": "*",
    "laravel/serializable-closure": "^2.0",
    "symfony/var-dumper": ">=4.2"
  },
  "suggest": {
    "ext-event": "For better performance.",
    "ext-zend-opcache": "To improve script execution speed.",
    "ext-redis": "For Redis-based caching or messaging."
  },
  "autoload": {
    "psr-4": {
      "T2\\": "./src",
      "App\\": "./src/App"
    },
    "files": [
      "src/App/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
